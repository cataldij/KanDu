{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "image": "latest"
      },
      "ios": {
        "simulator": false,
        "autoIncrement": true
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {},
    "preview": {
      "ios": {
        "appleId": "jcataldi@gmail.com",
        "ascAppId": "placeholder",
        "appleTeamId": "YOUR_TEAM_ID"
      }
    }
  }
}
